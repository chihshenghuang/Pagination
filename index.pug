doctype html
html
   head
      link(rel='stylesheet', type='text/css', href='style.css')
      script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js')
      script(src='main.js')
   body
      #paginate
      #page_navigation
      .search
         input#searchInput(type='search', placeholder='Search Page')
         input#searchSubmit(type='submit', value='Search')
      div
         label Items show per page
         select#page_per_show(name='page_per_show')
            option(selected='', disabled='') show per page
            option(value='1') 1
            option(value='2') 2
            option(value='3') 3
      div
         label Items number
         input#itemsNum(type='input')
         input#itemsNumInput(type='submit', value='Submit')



